{
  "name": "webpack-pure",
  "version": "1.0.0",
  "description": "practice webpack",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config ./config/webpack.config.js --env.currentEnv=development",
    "build": "webpack --config ./config/webpack.config.js --mode=production",
    "buildM": "webpack --config ./config/morePage.js --mode=development",
    "analyze": "webpack-bundle-analyzer ./analyze/stats.json",
    "stats": "webpack --profile --json > ./analyze/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NameHewei/webpack-pure.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "hewitt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NameHewei/webpack-pure/issues"
  },
  "homepage": "https://github.com/NameHewei/webpack-pure#readme",
  "dependencies": {
    "webpack": "^4.29.6",
    "webpack-dev-server": "^3.3.1"
  },
  "devDependencies": {
    "html-webpack-plugin": "^5.5.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0"
  }
}
